{
  "testEnvironment": "node",
  "setupFilesAfterEnv": ["<rootDir>/__tests__/setup.js"],
  "testMatch": [
    "**/__tests__/*.test.js"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "collectCoverageFrom": [
    "routes/config.js",
    "routes/drugs.js",
    "routes/companies.js"
  ],
  "coverageReporters": [
    "text",
    "lcov"
  ],
  "testTimeout": 10000,
  "verbose": true
}